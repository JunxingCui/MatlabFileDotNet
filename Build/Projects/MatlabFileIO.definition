<?xml version="1.0" encoding="utf-8"?>
<Project Name="MatlabFileIO" Path="./" Type="Library" Platforms="Android,iOS,Linux,MacOS,Windows,WindowsGL">
  <Properties>
    <NoWarn>1591</NoWarn>
    <RootNamespace>MatlabFileIO</RootNamespace>
    <PlatformSpecificOutputFolder>True</PlatformSpecificOutputFolder>
    <FrameworkVersions>
      <Platform Name="Android">
        <Version>v4.0.3</Version>
      </Platform>
    </FrameworkVersions>
    <CustomDefinitions>
      <Platform Name="Android">TRACE;ANDROID</Platform>
      <Platform Name="iOS">IOS</Platform>
      <Platform Name="Linux">TRACE;LINUX</Platform>
      <Platform Name="MacOS">MONOMAC</Platform>
      <Platform Name="Windows">TRACE;WINDOWS</Platform>
	  <Platform Name="WindowsGL">TRACE;WINDOWS</Platform>
    </CustomDefinitions>
  </Properties>
  <References>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
  </References>
  <Files>
    <Compile Include="MatlabFileArrayWriter.cs" />
    <Compile Include="MatlabFileHelper.cs" />
    <Compile Include="MatlabFileReader.cs" />
    <Compile Include="MatlabFileWriter.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
    <None Include="packages.config" />
  </Files>
</Project>
